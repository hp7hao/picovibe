version 42
__lua__
function _init()
	iplr()
end

function _update()
	uplr()
end

function _draw()
	cls()
	map()
	dplr()
end
-->8
--plr--

function iplr()
	plr={
		x=20,
		y=20,
		dx=0,
		f=false,
		state="idle",
		sp=1
	}
	
	jcd=0
	jforce=0
	gravity=3
end

function uplr()
	--movement--
	if btn(➡️) then
		plr.dx=2
		plr.f=false
		plr.state="run"
	elseif btn(⬅️) then
		plr.dx=-2
		plr.f=true
		plr.state="run"
	else
		plr.dx=0
		plr.state="idle"
	end
	plr.x+=plr.dx
	--mapborders--
	if plr.x<0 then
		plr.x=0
	end
	if plr.x>120 then
		plr.x=120
	end
	if plr.y<0 then
		plr.y=0
	end
	if plr.y>120 then
		plr.y=120
	end
	
	--jump--
	if btnp(❎) then
		if(onground()) then
			jcd=5
		end
	end
	if btn(❎) then
		if jcd>0 then
			jforce=9
			jcd-=1
		end
	end
	--gravity
	if not onground() then
		plr.y+=gravity
		plr.state="jump"
	end
	--fix intersecting w/floor
	for i=1,gravity do
		if inground() then
			plr.y-=1
		end
	end
	--apply jump force
	if jforce>0 then
		jforce-=1
	end
	plr.y-=jforce

	--animate plr--
	if plr.state=="idle" then
		plr.sp=1
	elseif plr.state=="run" then
		if plr.sp<4.7 then
			plr.sp+=.3
		else
			plr.sp=1
		end
	elseif plr.state=="jump" then
		if plr.dx==0 then
			plr.sp=1
		else
			plr.sp=2
		end
	end
end

function dplr()
	spr(plr.sp,plr.x,plr.y,1,1,plr.f)
end

function onground()
	local txl=(plr.x+2)/8
	local txr=(plr.x+4)/8
	local ty=(plr.y+8)/8
	
	if mget(txl,ty)==32 
		or mget(txr,ty)==32 then
		return true
	else
		return false
	end
end

function inground()
	local txl=(plr.x+4)/8
	local txr=(plr.x+4)/8
	local ty=(plr.y+7)/8
	
	if mget(txl,ty)==32 
		or mget(txr,ty)==32 then
		return true
	else
		return false
	end
end
