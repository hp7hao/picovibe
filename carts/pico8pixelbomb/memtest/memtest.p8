pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
pi_gpio = 0x5f80

pi_magic = {
 0x46c7.2002,
 0x6e44.ab77,
 0xd67f.dcbe,
 0x4d98.77d2,
}

magic=0
-- write pinput magic to gpio
function pi_init()
 for i = 1, #pi_magic do
  poke4(
   pi_gpio + 4 * (i - 1),
   pi_magic[i]
  )
 end
end

function _update()
 if btnp(4) then
 	pi_init()
 	magic=1
 end
end

function _draw()
	cls(0)
	if magic==1 then
		print("magic happen!")
	end
end
