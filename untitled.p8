pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
followers=0
function _init()
	cartdata("bilibilideck")
	followers=dget(0)
end

function _update()
	if btnp(4) then
		followers=followers+1
		dset(0,followers)
	end
	if btnp(5) then
		reload()
	end
end

function _draw()
	cls(0)
	print("local time: "..stat(93)..":"..stat(94)..":"..stat(95))
	print("date: "..stat(92).."/"..stat(91).."/"..stat(90))
	
	print("utc time: "..stat(83)..":"..stat(84)..":"..stat(85))
	print("date: "..stat(82).."/"..stat(81).."/"..stat(80))
	print("bilibili followers: "..followers)
end
